{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "isolatedModules": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@/*": ["./resources/js/*"],
      "&/*": ["./resources/svg/*"]
    }
  }
}
