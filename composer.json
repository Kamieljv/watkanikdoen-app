{
    "name": "watkanikdoen/watkanikdoen-app",
    "description": "Watkanikdoen.nl application",
    "keywords": ["watkanikdoen", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.4.0",
        "artesaos/seotools": "^1.3",
        "blade-ui-kit/blade-icons": "^1.8",
        "codeat3/blade-ant-design-icons": "^1.9",
        "codeat3/blade-clarity-icons": "^1.10",
        "darkaonline/l5-swagger": "^8.6",
        "dotswan/filament-map-picker": "2.1.2",
        "filament/filament": "^4.4",
        "guzzlehttp/guzzle": "^7.10",
        "intervention/image": "^3.11",
        "lab404/laravel-impersonate": "^1.7",
        "laravel/framework": "^12.0",
        "laravel/helpers": "^1.8",
        "laravel/tinker": "^2.11",
        "laravel/ui": "^4.6",
        "livewire/livewire": "^3.7",
        "matanyadaev/laravel-eloquent-spatial": "^4.5",
        "mcordingley/linearalgebra": "^3.0",
        "mobiledetect/mobiledetectlib": "^4.8",
        "orangehill/iseed": "^3.1",
        "spatie/icalendar-generator": "^3.2",
        "spatie/laravel-permission": "^6.24",
        "stevebauman/purify": "^6.3",
        "tomatophp/filament-media-manager": "^4.0",
        "tymon/jwt-auth": "^2.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16",
        "fakerphp/faker": "^1.24",
        "nunomaduro/collision": "^8.8",
        "spatie/laravel-sitemap": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@php artisan filament:upgrade"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "pestphp/pest-plugin": true
        }
    }
}
